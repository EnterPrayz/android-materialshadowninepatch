<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ms9_DefaultShadowStyle">
        <item name="ms9_ambientShadowDrawablesList">@array/ms9_ambient_shadow_drawables</item>
        <item name="ms9_spotShadowDrawablesList">@array/ms9_spot_shadow_drawables</item>
        <item name="ms9_forceUseCompatShadow">false</item>
        <item name="ms9_affectsDisplayedPosition">true</item>
        <item name="ms9_useAmbientShadow">true</item>
        <item name="ms9_useSpotShadow">true</item>
    </style>

    <style name="ms9_DefaultShadowStyleCompatOnly" parent="ms9_DefaultShadowStyle">
        <item name="ms9_forceUseCompatShadow">true</item>
    </style>

    <style name="ms9_NoDisplayedPositionAffectShadowStyle">
        <item name="ms9_ambientShadowDrawablesList">@array/ms9_ambient_shadow_drawables</item>
        <item name="ms9_spotShadowDrawablesList">@array/ms9_spot_shadow_drawables</item>
        <item name="ms9_forceUseCompatShadow">false</item>
        <item name="ms9_affectsDisplayedPosition">false</item>
        <item name="ms9_useAmbientShadow">true</item>
        <item name="ms9_useSpotShadow">true</item>
    </style>

    <style name="ms9_NoDisplayedPositionAffectShadowStyleCompatOnly" parent="ms9_NoDisplayedPositionAffectShadowStyle">
        <item name="ms9_forceUseCompatShadow">true</item>
    </style>

    <style name="ms9_CompositeShadowStyle">
        <item name="ms9_ambientShadowDrawablesList">@array/ms9_composite_shadow_drawables</item>
        <item name="ms9_spotShadowDrawablesList">@null</item>
        <item name="ms9_forceUseCompatShadow">false</item>
        <item name="ms9_affectsDisplayedPosition">false</item>
        <item name="ms9_useAmbientShadow">true</item>
        <item name="ms9_useSpotShadow">false</item>
    </style>

    <style name="ms9_CompositeShadowStyleCompatOnly" parent="ms9_CompositeShadowStyle">
        <item name="ms9_forceUseCompatShadow">true</item>
    </style>

</resources>
